package dao.impl;
import java.util.List;

import javax.persistence.*;

import base.*;
import dao.*;
import dto.*;
public class EtapaImpl  extends JPA implements DAO<Etapa>{
	
	@Override
	public Etapa getById(Long id) {
		Etapa result = getEntityManager().find(Etapa.class, id);
		closeEntityManager();
		return result;
	}
}
